{
  "name": "Fullstack Open - Dev",
  "dockerComposeFile": ["./todo-app/todo-backend/docker-compose.yml"],
  "service": "app",
  "workspaceFolder": "/workspace/todo-app/todo-backend",
  "runServices": ["app"],
  "shutdownAction": "stopCompose",
  "forwardPorts": [3000],
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-azuretools.vscode-docker",
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode"
      ]
    }
  },
  "postCreateCommand": "npm install",
  "remoteUser": "vscode"
}